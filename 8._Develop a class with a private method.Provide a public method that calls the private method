using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _8 question
{
    class que
    {
       
        private void MethodA()
        {
            Console.WriteLine("This is a private method.");
        }

       
        public void MethodB()
        {
            Console.WriteLine("This is a public method.");
            MethodA(); 
        }
    }

    class Program
    {
        static void Main()
        {
            
            que Obj = new que();

            
            Obj.MethodB();
            Console.Read();

        }
    }
}
